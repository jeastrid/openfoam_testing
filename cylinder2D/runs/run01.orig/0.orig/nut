FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.012245;

boundaryField
{
    "(inlet|outlet|topBottom)"
    {
        type            calculated;
        value           $internalField;
    }

    cylinder
    {
        type            nutkWallFunction;
        value           $internalField;
    }

    frontBack
    {
        type            empty;
    }
}


